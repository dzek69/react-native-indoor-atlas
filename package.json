    "@babel/register": "^7.0.0",
    "@dzek69/eslint-config-base": "^0.0.1",
    "babel-plugin-rewire": "^1.2.0",
    "cross-env": "^5.2.0",
    "docdash": "^1.0.0",
    "eslint": "^5.9.0",
    "fs-extra": "^7.0.1",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "mockery": "^2.1.0",
    "must": "^0.13.4"
  },
  "peerDependencies": {
    "react-native": ">=0.57.0"
  },
  "keywords": [
    "react-native",
    "indoor-atlas"
