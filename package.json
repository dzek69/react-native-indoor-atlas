    "lint": "eslint src/**/*.js src/*.js",
    "lint:fix": "npm run lint -- --fix"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/polyfill": "^7.0.0",
